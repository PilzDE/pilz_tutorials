<?xml version="1.0"?>

<!-- %Tag(FULLTEXT)% -->
<launch>
  <!-- %Tag(PSEN_SCAN_NODE)% -->
  <node name="laser_scanner" type="psen_scan_v2_node" pkg="psen_scan_v2" output="screen" required="true">
    <rosparam command="load" file="$(find psen_scan_v2_tutorials)/config/scanner_config.yaml" />
  </node>
  <!-- %EndTag(PSEN_SCAN_NODE)% -->
  <!-- %Tag(RVIZ)% -->
  <node name="$(anon rviz)" type="rviz" pkg="rviz" args="-d $(find psen_scan_v2)/config/config.rviz" />
  <!-- %EndTag(RVIZ)% -->
</launch>
<!-- %EndTag(FULLTEXT)% -->
